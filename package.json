    "compression": "^1.7.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "react": "^16.3.0",
    "react-apollo": "2.1.2",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.1",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
