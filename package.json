    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.13",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^0.3.1",
    "webpack-stylish": "^0.1.8"
